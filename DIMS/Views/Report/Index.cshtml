@model EnterpriseInfoListViewModel
@{
    Layout = "_layout";
}
<div>
    <section class="content">
        <div class="container-fluid">
            <div class="row">
                <div class="col-12">
                    <div class="card">
                        <div class="card-header">
                            <button type="button" class="btn btn-lg btn-outline-primary float-right font-weight-bolder" data-toggle="modal" data-target="#modal-md">
                                <i class="fa fa-plus-square mr-2"></i> Add New
                            </button>
                            <h2 class="float-left note-fontsize-10 font-weight-bolder">Enterprise Info List</h2>
                        </div>

                        <div class="card-body">
                            <table id="example1" class="table table-bordered table-hover">
                                <thead>
                                    <tr class="bg-gradient-lightblue text-center">
                                        <th rowspan="2" class="align-middle">Maqaa Intarpiraayizoota</th>
                                        <th colspan="5">Teessoo</th>
                                        <th colspan="3" class="align-middle">baay'ina miseensaa qabu</th>
                                        <th colspan="3" class="align-middle">Umurii(18-34)</th>
                                        <th colspan="3" class="align-middle">Sadarkaa Barumsaa</th>
                                        <th colspan="3" class="align-middle">Sadarkaa Barumsaa</th>
                                        <th rowspan="2" class="align-middle">Bara Hundeeffaamaa</th>
                                        <th rowspan="2" class="align-middle">Damee hojii</th>
                                        <th rowspan="2" class="align-middle">Gosa hojii</th>
                                        <th rowspan="2" class="align-middle">Sadarkaa interpiraayizichaa</th>
                                        <th rowspan="2" class="align-middle">Gosa Gurmii</th>
                                        <th rowspan="2" class="align-middle">Lakk. TIN</th>
                                        <th rowspan="2" class="align-middle">Sadarkaa guddinaa</th>
                                        <th rowspan="2" class="align-middle">Ka-umsa Kaappitaalaa</th>
                                        <th rowspan="2" class="align-middle">Madda ka'umsa Kappitaalaa </th>

                                        <th rowspan="2" class="align-middle">Action</th>
                                    </tr>
                                    <tr class="text-center">
                                        <th>Godina</th>
                                        <th>Magaalaa</th>
                                        <th>Gandaa</th>
                                        <th>Lakk. Manaa</th>
                                        <th>Lakk. Bilbilaa</th>
                                        <th>Dhi.</th>
                                        <th>Dub.</th>
                                        <th>Ida.</th>
                                        <th>Dhi.</th>
                                        <th>Dub.</th>
                                        <th>Ida.</th>
                                        <th>Koleejii Dhi.</th>
                                        <th>Koleejii Dub.</th>
                                        <th>Koleejii Ida.</th>
                                        <th>University Dh.</th>
                                        <th>University Dub.</th>
                                        <th>University Ida.</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in ViewBag.GetAllReportList)
                                    {
                                    <tr class="info">
                                        <td>@item.EnterpriseName</td>
                                        <td>@item.ZoneName</td>
                                        <td>@item.CityName</td>
                                        <td>@item.KebeleName</td>
                                        <td>@item.HouseNumber</td>
                                        <td>@item.PhoneNumber</td>
                                        <td>@(item.MaleFouder)</td>
                                        <td>@(item.FemaleFounder)</td>
                                        <td>@(item.MaleFouder + item.FemaleFounder)</td>
                                        <td>@(item.MaleFouderYear)</td>
                                        <td>@(item.FemaleFounderYear)</td>
                                        <td>@(item.MaleFouderYear + item.FemaleFounderYear)</td>
                                        <td>@(item.MaleCollege)</td>
                                        <td>@(item.FemaleCollege)</td>
                                        <td>@(item.MaleCollege + item.FemaleCollege)</td>
                                        <td>@(item.MaleUniversity)</td>
                                        <td>@(item.FemaleUniversity)</td>
                                        <td>@(item.MaleUniversity + item.FemaleUniversity)</td>
                                        <td>@item.EstableshedYear</td>
                                        <td>@item.BranchName</td>
                                        <td>@item.BranchItemName</td>
                                        <td>@item.EnterpriseLevelName</td>
                                        <td>@item.GroupTypeName</td>
                                        <td>@item.TINNumber</td>
                                        <td>@item.PromotionLevelName</td>
                                        <td>@item.InitialCapital</td>
                                        <td>@item.SourceCapitalName</td>
                                        <td>
                                            Kassahun
                                            @*<button type="button" class="btn btn-lg btn-outline-info font-weight-bolder text-lg-center fa fa-edit mr-2" data-toggle="ajax-modal" title="Edit Services" data-target="#EditRegionmodal" data-url="@Url.Action($"Edit/{item.Id}")"></button>
            <a class="btn btn-lg btn-outline-danger font-weight-bolder text-lg-center fa fa-trash-alt" title="Delete Region" asp-action="DeleteRegion" asp-controller="Region" asp-route-id="@item.Id" onclick="return confirm('Are you sure you want to delete this?')"></a>*@
                                        </td>
                                    </tr>
                                    }
                                </tbody>
                                <tfoot>
                                    <tr class="bg-gradient-navy">
                                        <th colspan="28"></th>
                                    </tr>
                                </tfoot>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
</div>